<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="acount_move_form_ext_tracking_number" model="ir.ui.view">
            <field name="name">account.move.form.ext.tracking.number</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//group//field[@name='payment_reference']" position="after">
           
                    <field name="tracking_number" widget="Char"/>
    
                </xpath>  
            </field>
        </record>
    </data>
</odoo>